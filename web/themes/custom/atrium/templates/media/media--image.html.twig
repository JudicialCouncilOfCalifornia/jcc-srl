{#
/**
 * @file
 * Template for "Image" media.
 */
#}
{% set classes = [
  'media-library--image',
] %}

<div{{ attributes.addClass(classes) }}>
  {% if media.field_media_image.0 %}
    {{ content|without('name', 'field_media_svg') }}
  {% else %}
    {{ content|without('name', 'field_media_image', 'thumbnail') }}
  {% endif %}
</div>
