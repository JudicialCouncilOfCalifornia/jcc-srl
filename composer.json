        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "components/highlightjs": "9.7.*",
        "d3/d3": "3.5.*",
        "drupal/log_stdout": "^1.1",
        "drupal/search_kint": "^1.0",
        "drupal/stage_file_proxy": "^1.0@RC",
        "drupal/twig_xdebug": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "oomphinc/composer-installers-extender": true,
            "zaporylie/composer-drupal-optimizations": true,
            "joachim-n/composer-manifest": true,
            "drupal/console-extend-plugin": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::postInstall",
            "find ./vendor/ -type d -name .git | xargs rm -rf",
            "find ./web/ -type d -name .git | xargs rm -rf",
            "find .git/ -type d -name hooks | xargs rm -rf"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::postInstall",
            "find ./vendor/ -type d -name .git | xargs rm -rf",
            "find ./web/ -type d -name .git | xargs rm -rf",
            "find .git/ -type d -name hooks | xargs rm -rf"
        ],
        "post-drupal-scaffold-cmd": [
            "patch -p1 < patches/gitattributes.patch",
            "patch -p1 < patches/robots.patch",
            "patch -p1 < patches/robots-2.patch",
            "patch -p1 < patches/robots-3.patch",
            "patch -p1 < patches/robots-4.patch",
            "patch -p1 < patches/robots-5.patch",
            "patch -p1 < patches/robots-6.patch",
            "patch -p1 < patches/robots-7.patch",
            "patch -p1 < patches/robots-8.patch"
        ],
        "build-assets": [
            "@prepare-for-pantheon",
            "composer install --optimize-autoloader --no-dev"
        ],
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": "echo 'No code sniff step defined.'",
        "unit-test": "echo 'No unit test step defined.'",
        "artifact-sync": [
            "rsync -KPar --delete --force --files-from=scripts/deploy.txt ./ data/artifact/"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Don't override media title with metadata on translation save.": "https://www.drupal.org/files/issues/2019-04-25/media-translation_hassourcefieldchanged-2999370-11.patch",
                "Media library edit": "https://www.drupal.org/files/issues/2021-01-04/3168868-9.patch",
                "Media name reverts to default": "https://www.drupal.org/files/issues/2020-10-28/3157183_20.patch"
            },
            "drupal/webform": {
                "By default, do not save submissions.": "patches/disable-results-by-default.patch"
            },
            "drupal/svg_image_field": {
                "Fix previews": "https://www.drupal.org/files/issues/2019-05-06/svg_image_field-zero-width-preview-3053018.patch"
            },
            "drupal/fillpdf": {
                "Secure user data after pdf is rendered.": "patches/webform-pdf-protect.patch"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false
            }
        }
    }
}
